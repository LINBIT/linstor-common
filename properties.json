{
        "properties": {
                "stordriver_volumegroup": {
                        "key": [
                                "NAMESPC_STORAGE_DRIVER",
                                "KEY_STOR_POOL_VOLUME_GROUP"
                        ],
                        "type": "regex",
                        "value": "[a-zA-Z0-9_-]+",
                        "internal": true
                },
                "stordriver_thinpool": {
                        "key": [
                                "NAMESPC_STORAGE_DRIVER",
                                "KEY_STOR_POOL_THIN_POOL"
                        ],
                        "type": "regex",
                        "value": "[a-zA-Z0-9_-]+",
                        "internal": true
                },
                "stordriver_zpool": {
                        "key": [
                                "NAMESPC_STORAGE_DRIVER",
                                "KEY_STOR_POOL_ZPOOL"
                        ],
                        "type": "regex",
                        "value": "[a-zA-Z0-9_/-]+",
                        "internal": true
                },
                "stordriver_swordfish_host_port": {
                        "key": [
                                "NAMESPC_STORAGE_DRIVER",
                                "KEY_STOR_POOL_SF_HOST_PORT"
                        ],
                        "type": "regex",
                        "value": ".*",
                        "internal": true
                },
                "stordriver_swordfish_stor_service": {
                        "key": [
                                "NAMESPC_STORAGE_DRIVER",
                                "KEY_STOR_POOL_SF_STOR_SVC"
                        ],
                        "type": "regex",
                        "value": ".*",
                        "internal": true
                },
                "stordriver_swordfish_stor_pool": {
                        "key": [
                                "NAMESPC_STORAGE_DRIVER",
                                "KEY_STOR_POOL_SF_STOR_POOL"
                        ],
                        "type": "regex",
                        "value": ".*",
                        "internal": true
                },
                "stordriver_swordfish_user_name": {
                        "key": [
                                "NAMESPC_STORAGE_DRIVER",
                                "KEY_STOR_POOL_SF_USER_NAME"
                        ],
                        "type": "regex",
                        "value": ".*",
                        "internal": true
                },
                "stordriver_swordfish_user_passwd": {
                        "key": [
                                "NAMESPC_STORAGE_DRIVER",
                                "KEY_STOR_POOL_SF_USER_PW"
                        ],
                        "type": "regex",
                        "value": ".*",
                        "internal": true
                },
                "stordriver_swordfish_poll_timeout_vlm_crt": {
                        "key": [
                                "NAMESPC_STORAGE_DRIVER",
                                "KEY_STOR_POOL_SF_POLL_TIMEOUT_VLM_CRT"
                        ],
                        "type": "regex",
                        "value": "[0-9]+",
                        "internal": true
                },
                "stordriver_swordfish_poll_retries_vlm_crt": {
                        "key": [
                                "NAMESPC_STORAGE_DRIVER",
                                "KEY_STOR_POOL_SF_POLL_RETRIES_VLM_CRT"
                        ],
                        "type": "regex",
                        "value": "[0-9]+",
                        "internal": true
                },
                "stordriver_swordfish_poll_timeout_attach_vlm": {
                        "key": [
                                "NAMESPC_STORAGE_DRIVER",
                                "KEY_STOR_POOL_SF_POLL_TIMEOUT_ATTACH_VLM"
                        ],
                        "type": "regex",
                        "value": "[0-9]+",
                        "internal": true
                },
                "stordriver_swordfish_poll_retries_attach_vlm": {
                        "key": [
                                "NAMESPC_STORAGE_DRIVER",
                                "KEY_STOR_POOL_SF_POLL_RETRIES_ATTACH_VLM"
                        ],
                        "type": "regex",
                        "value": "[0-9]+",
                        "internal": true
                },
                "stordriver_swordfish_poll_timeout_grep_nvme_uuid": {
                        "key": [
                                "NAMESPC_STORAGE_DRIVER",
                                "KEY_STOR_POOL_SF_POLL_TIMEOUT_GREP_NVME_UUID"
                        ],
                        "type": "regex",
                        "value": "[0-9]+",
                        "internal": true
                },
                "stordriver_swordfish_poll_retries_grep_nvme_uuid": {
                        "key": [
                                "NAMESPC_STORAGE_DRIVER",
                                "KEY_STOR_POOL_SF_POLL_RETRIES_GREP_NVME_UUID"
                        ],
                        "type": "regex",
                        "value": "[0-9]+",
                        "internal": true
                },
                "stordriver_swordfish_composed_node_name": {
                        "key": [
                                "NAMESPC_STORAGE_DRIVER",
                                "KEY_STOR_POOL_SF_COMPOSED_NODE_NAME"
                        ],
                        "type": "regex",
                        "value": ".*",
                        "internal": true
                },
                "storage_pool_name": {
                        "key": [
                                "KEY_STOR_POOL_NAME"
                        ],
                        "type": "regex",
                        "value": "^[a-zA-Z0-9_][a-zA-Z0-9_-]{2,47}$"
                },
                "preferred_nic": {
                        "key": [
                                "KEY_STOR_POOL_PREF_NIC"
                        ],
                        "type": "regex",
                        "value": "^[a-zA-Z0-9_][a-zA-Z0-9_-]{2,47}$",
                        "info": "Preferred network interface to use"
                },
                "tcp_port_range": {
                        "key": [
                                "KEY_TCP_PORT_AUTO_RANGE"
                        ],
                        "type": "regex",
                        "value": "[0-9]+-[0-9]+",
                        "info": "Range for auto-allocation of resource TCP ports"
                },
                "minor_nr_range": {
                        "key": [
                                "KEY_MINOR_NR_AUTO_RANGE"
                        ],
                        "type": "regex",
                        "value": "[0-9]+-[0-9]+",
                        "info": "Range for auto-allocation of volume minor numbers"
                },
                "peer_slots_new_resource": {
                        "key": [
                                "KEY_PEER_SLOTS_NEW_RESOURCE"
                        ],
                        "type": "range",
                        "min": 1,
                        "max": 31,
                        "info": "DRBD peer slots to allocate for newly created resources (default 7), the number of peer slots cannot be changed once the resource is created, so allow sufficient slots to increase redundancy in the future"
                },
                "peer_slots": {
                        "key": [
                                "KEY_PEER_SLOTS"
                        ],
                        "type": "range",
                        "min": 1,
                        "max": 31,
                        "info": "DRBD peer slots present on resource",
                        "internal": true
                },
                "dm_stats": {
                        "key": [
                                "NAMESPC_STORAGE_DRIVER",
                                "KEY_DMSTATS"
                        ],
                        "type": "boolean",
                        "info": "Enable dmstats on lvm create"
                },
                "override_vlm_id": {
                        "key": [
                                "KEY_STOR_POOL_OVERRIDE_VLM_ID"
                        ],
                        "type": "regex",
                        "value": ".*",
                        "info": "Overwrites the generated '<resource-name>_<5 digit volume number>' volume name for migration purposes."
                }
        },
        "objects": {
                "controller": [
                        "tcp_port_range",
                        "minor_nr_range",
                        "peer_slots_new_resource",
                        "dm_stats",
                        "stordriver_swordfish_user_name",
                        "stordriver_swordfish_user_passwd",
                        "stordriver_swordfish_host_port"
                ],
                "node": [
                        "storage_pool_name",
                        "stordriver_swordfish_composed_node_name",
                        "stordriver_swordfish_user_name",
                        "stordriver_swordfish_user_passwd",
                        "stordriver_swordfish_host_port"
                ],
                "storagepool-definition": [
                ],
                "storagepool": [
                        "stordriver_volumegroup",
                        "stordriver_thinpool",
                        "stordriver_zpool",
                        "preferred_nic",
                        "stordriver_swordfish_stor_service",
                        "stordriver_swordfish_stor_pool",
                        "stordriver_swordfish_poll_timeout_vlm_crt",
                        "stordriver_swordfish_poll_retries_vlm_crt",
                        "stordriver_swordfish_poll_timeout_attach_vlm",
                        "stordriver_swordfish_poll_retries_attach_vlm",
                        "stordriver_swordfish_poll_timeout_grep_nvme_uuid",
                        "stordriver_swordfish_poll_retries_grep_nvme_uuid"
                ],
                "resource-definition": [
                        "storage_pool_name",
                        "peer_slots_new_resource"
                ],
                "resource": [
                        "storage_pool_name",
                        "preferred_nic",
                        "peer_slots"
                ],
                "volume-definition": [
                        "storage_pool_name",
                        "override_vlm_id"
                ]
        }
}
